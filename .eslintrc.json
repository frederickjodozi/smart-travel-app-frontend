{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "_baseUrl",
          "_getResponseData"
        ]
      }
    ],
    "class-methods-use-this": "off"
  }
}
